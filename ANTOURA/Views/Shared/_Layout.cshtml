
<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>

    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/jqueryval")

    <script>
        $(function () {
            var icons = {
                header: "defaultIcon",
                activeHeader: "selectedIcon"
            };
            $("#accordion").accordion({ icons: icons, collapsible: true });


        });
    </script>
</head>

<body>
  
    <!-- wrapper -->
    <div class="wrapper">
        <!-- HEADER -->
        <header class="mainheader">
            <div class="logosis"> <img src="@Url.Content("~/content/images/SIS LOGO.png")" /> </div>

            <a href="www.college-antoura.edu.lb" class="logoantoura">
                <img src="@Url.Content("~/content/images/ANTOURA LOGO.png")" />
            </a>

            <div class="language">EN - FR </div>
        </header>
        <!-- END HEADER -->


        <div class="content">

            <div class="leftcontent">
                <div class="class">
                    <div id="accordion">
                        <h3>CLASSE</h3>
                        <p>
                            6A
                        </p>
                      
                    </div>
                </div>
            </div>

            <div class="rightcontent">
                @{Html.RenderPartial("_Welcome");}
               <div class="hr1">
                   
               </div>
                <div class="hr2">

                </div>
                <nav class="nav1">
    <ul id="menu">
        <li>@Html.ActionLink("AGENDA", "index", "Agenda")</li>
        <li>|</li>
        <li>@Html.ActionLink("CONTROLE", "Index", "Examen")</li>
        <li>|</li>
        <li>@Html.ActionLink("LIVRET", "Contact", "Home")</li>
        
    </ul>
</nav>
                
                    @RenderBody()
                
                   
              

            </div>



        </div>

    </div>

    @RenderSection("Scripts", required: false)

</body>
